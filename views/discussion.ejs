<%- include('partials/header') %>
<%- include('partials/navbar') %>

<div class=" container-fluid network-image" style=" padding:5% 15% 0% 15%; margin:0;">
  <img src="images/1.PNG" class="network" alt="" style=" width : 15%; height:auto;">
  <p class="des-head" style=" font-size:2.5rem; ">MANITora</p>
</div>



<%  for(var i=0;i<questions.length;i++){ %>
<div class="query-box">
  <img src="images/profile.png" alt="">
  <div class="" style=" display:inline-block; margin:0; padding:0;">
    <p class="name"><%=questions[i].postedBy.name %></p>
    <p class="branch"><%=questions[i].postedBy.college +" "+questions[i].postedBy.branch%></p>
  </div>
  <p class="quest"><%=questions[i].quest %></p>
  <div class="row">
    <div class="col-sm-4">
      <button type="button" class="btn btn-sm">
        <p class="action">Like</p>
        <i class="fas fa-thumbs-up action"></i>
      <p class="action number">5</p>
      </button>

    </div>
    <div class="col-sm-4">
      <p class="action">Dislike</p>
      <i class="fas fa-thumbs-down action"></i>
      <p class="action number">5</p>
    </div>
    <div class="col-sm-4">
      <p class="action">Answers</p>
      <i class="far fa-comment action"></i>
      <p class="action number">5</p>
    </div>

  </div>
</div>
<% } %>







<%- include('partials/footer') %>
